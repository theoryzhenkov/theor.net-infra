# Template for secrets.yaml
# After updating .sops.yaml with your server's age public key, run:
#   nix-shell -p sops
#   cp secrets/secrets.yaml.template secrets/secrets.yaml
#   sops secrets/secrets.yaml
# This will open an editor. Replace the values below, save, and the file will be encrypted.

ghcr_username: YOUR_GITHUB_USERNAME
ghcr_token: ghp_YOUR_CLASSIC_PAT_HERE